#include <iostream>

#include <stdio.h>
#define HAUTMAX 10
#define LARGEMAX 10
void affichegrille(char grille[HAUTMAX][LARGEMAX]);

int main(void) {
	int h, l;
	char grille[HAUTMAX][LARGEMAX];
	

	for (h = 0; h < HAUTMAX; ++h) {
		for (l = 0; l < LARGEMAX; ++l) {
			
			grille[h][l] = 'O';

		}
	}


	affichegrille(grille);
	return 0;
}


void affichegrille(char grille[HAUTMAX][LARGEMAX]) {
	int h, l;
	for (h = 0; h < HAUTMAX; ++h) {
		for (l = 0; l < LARGEMAX; ++l) printf("%c", grille[h][l]);
		printf("\n");
	}

}
